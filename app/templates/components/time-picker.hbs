<div class="time-picker">
    <div class="time-picker-col">
        <div class="daticon">
          {{#each selectedDates as |event index|}}
              <div class="daticon">
                <div class="dow">
                  {{moment-format event 'ddd'}}
                </div>
                <div class="day">
                  {{moment-format event 'D'}}
                </div>
                <div class="month">
                  {{moment-format event 'MM'}}
                </div>
                <span class="delete" {{action removeDate event}}></span>
              </div>
          {{/each}}
        </div>
    </div>
    <div class="time-picker-col" ng-repeat="time in date.times track by $index">
        <input type="text" class="time-picker-input" time-picker ng-model="time" ng-model-options="{ updateOn: 'blur' }" />
    </div>
</div>
{{yield}}
