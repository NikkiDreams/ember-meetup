{
  "name": "ember-meetup",
  "version": "0.0.0",
  "description": "Collaboratobve Scheduling in Ember",
  "author": "Nichole 'Nix' Shannon",
  "url": "http://nichole-shannon.us",
  "repository": "https://github.com/NikkiDreams/ember-meetup",
  "private": false,
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "prestart": "NODE_ENV=development; DEBUG=ember-meetup:* babel-node ./api_server/app.js &",
