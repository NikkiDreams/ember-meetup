    "reload": "pm2 reload \"api\"",
    "stopserver": "pm2 stop \"api\"",
    "testserver": "NODE_ENV=TEST mocha; npm run lint && npm test",
    "test": "ember test",
    "coverage": "NODE_ENV=TEST istanbul cover _mocha",
    "lint": "eslint --ext .js api_server/ lib/ index.js"
  },
